{
  "expo": {
    "name": "流畅阅读",
    "slug": "flowread",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.qgming.flowread"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.qgming.flowread"
    },
    "web": {
      "favicon": "./assets/images/splash-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "449a196b-e504-4015-aee0-f142f66d98ed"
      }
    },
    "owner": "qgming",
    "scheme": "flowread",
    "plugins": [
      "expo-font"
    ]
  }
}
